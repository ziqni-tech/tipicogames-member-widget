<div class="cl-main-widget-reward-header">
  <div class="cl-main-widget-reward-header-back"></div>
  <div class="cl-main-widget-reward-header-label">{{ headerLabel }}</div>
  <div class="cl-main-widget-reward-header-close"></div>
</div>
<div class="cl-main-widget-reward-details">
  <div class="cl-main-widget-reward-details-info">
    <div class="cl-main-widget-reward-details-info-icon"></div>
  </div>
  <div class="cl-main-widget-reward-details-content">
    <div class="cl-main-widget-reward-details-content-label"></div>
    <div class="cl-main-widget-reward-details-content-date"></div>
  </div>
</div>
<div class="cl-main-widget-reward-list">
  <div class="cl-main-widget-reward-list-body">
    <div class="cl-main-widget-reward-list-body-res"></div>
  </div>
</div>
<div class="cl-main-widget-reward-details-container">
  <div class="cl-main-widget-reward-details-wrapper">
    <div class="cl-main-widget-reward-details-header">
      <a class="cl-main-widget-reward-details-back-btn"></a>
      <div class="cl-main-widget-reward-details-close"></div>
    </div>
    <div class="cl-main-widget-reward-details-body-container">
      <div class="cl-main-widget-reward-details-value">-</div>
      <div class="cl-main-widget-reward-details-type">-</div>
      <div class="cl-main-widget-reward-details-expires">{{ expiresInLabel }} <span class="cl-main-widget-reward-details-expires-value">-</span></div>

      <div class="cl-main-widget-ach-details-games">
        <div class="cl-main-widget-ach-details-games-label">{{ gamesLabel }}</div>
        <div class="cl-main-widget-ach-details-game-items reward-games"></div>
        <div class="cl-main-widget-ach-details-game-full"></div>
        <div class="cl-main-widget-ach-details-game-overlay"></div>
      </div>

      <div class="cl-main-widget-reward-details-content-wrapp">
        <div class="cl-main-widget-reward-details-content-header">
          {{ freeSpinDetailsLabel }} <div class="cl-main-widget-reward-details-content-wrapp-expand expand-item"></div>
        </div>
        <div class="cl-main-widget-reward-details-content-body">
          <div class="cl-main-widget-reward-details-content-body-item">
            <div class="cl-main-widget-reward-details-content-body-item-label">{{ totalLabel }}</div>
            <div class="cl-main-widget-reward-details-content-body-item-value reward">-</div>
          </div>
<!--          <div class="cl-main-widget-reward-details-content-body-item">-->
<!--            <div class="cl-main-widget-reward-details-content-body-item-label">Credited on</div>-->
<!--            <div class="cl-main-widget-reward-details-content-body-item-value">-</div>-->
<!--          </div>-->
          <div class="cl-main-widget-reward-details-content-body-item">
            <div class="cl-main-widget-reward-details-content-body-item-label">{{ expiresOnLabel }}</div>
            <div class="cl-main-widget-reward-details-content-body-item-value expires">-</div>
          </div>
          <div class="cl-main-widget-reward-details-content-body-item">
            <div class="cl-main-widget-reward-details-content-body-item-label">{{ campaignLabel }}</div>
            <div class="cl-main-widget-reward-details-content-body-item-value campaign">-</div>
          </div>
        </div>
      </div>

      <div class="cl-main-widget-reward-details-tc-wrapp">
        <div class="cl-main-widget-reward-details-tc-header">
          {{ tAndCLabel }} <div class="cl-main-widget-reward-details-tc-wrapp-expand expand-item"></div>
        </div>
        <div class="cl-main-widget-reward-details-tc-body"></div>
      </div>

      <div class="cl-main-widget-reward-details-forfeit">{{ drawerForfeitBtn }}</div>
    </div>
  </div>
</div>

<div class="cl-main-widget-reward-details-drawer">
  <div class="cl-main-widget-reward-details-drawer-icon"></div>
  <div class="cl-main-widget-reward-details-drawer-title">{{ drawerLabel }}</div>
  <div class="cl-main-widget-reward-details-drawer-description">{{ drawerDescription }}</div>
  <div class="cl-main-widget-reward-details-drawer-btn-claim">{{ drawerKeepBtn }}</div>
  <div class="cl-main-widget-reward-details-drawer-btn-decline">{{ drawerForfeitBtn }}</div>
</div>